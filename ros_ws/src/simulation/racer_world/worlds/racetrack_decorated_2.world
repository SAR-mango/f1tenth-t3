<?xml version="1.0" encoding="UTF-8"?>
<sdf version="1.8">
  <world name="racetrack">

        <include>
            <uri>https://fuel.gazebosim.org/1.0/OpenRobotics/models/Sun</uri>
        </include>

    <physics type="ode" default="true">
      <!-- Default physics profile with no changes -->
    </physics>

            <link name='chassis'>
                <pose>-0.030113 0 0.030771 0 -0 0</pose>
                <inertial>
                <pose>0 0 -0.05 0 0 0</pose>
                <mass>1.0</mass>
                <inertia>
                    <ixx>0.126164</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>0.416519</iyy>
                    <iyz>0</iyz>
                    <izz>0.481014</izz>
                </inertia>
                </inertial>
                <visual name='visual'>
                <geometry>
                    <box>
                    <size>0.4 0.2 0.1</size>
                    </box>
                </geometry>
                <material>
                    <ambient>0.5 0.5 1.0 1</ambient>
                    <diffuse>0.5 0.5 1.0 1</diffuse>
                    <specular>0.0 0.0 1.0 1</specular>
                </material>
                </visual>
                <collision name='collision'>
                <geometry>
                    <box>
                    <size>0.4 0.2 0.1</size>
                    </box>
                </geometry>
                </collision>

                <!-- Visual Lidar: a visible “puck” so you can see the sensor mounted -->
                <visual name="lidar_visual">
                <pose relative_to="lidar_frame">0 0 -0.03 0 0 0</pose>
                <geometry>
                    <cylinder>
                    <radius>0.03</radius>
                    <length>0.04</length>
                    </cylinder>
                </geometry>
                </visual>

    <!-- Removed the standalone Hokuyo Fuel include to prevent the 2nd lidar -->

    <gui>
      <camera name="my_camera">
        <pose>-3.2 7.8 31 0 1.5707963 1.5707963</pose>
      </camera>
    </gui>

    <!-- ========================= TRACK ========================= -->
    <model name="track">
      <pose>0 0 0.01 0 0 0</pose>
      <static>true</static>

      <link name="track">
        <visual name="visual">
          <geometry>
            <mesh>
              <uri>../meshes/track_decorated_2.dae</uri>
            </mesh>
          </geometry>
        </visual>

        <collision name="collision">
          <geometry>
            <plane>
              <normal>0 0 1</normal>
              <size>100 100</size>
            </plane>
          </geometry>
          <surface>
            <friction>
              <ode>
                <mu>100</mu>
                <mu2>50</mu2>
              </ode>
            </friction>
          </surface>
        </collision>
      </link>

      <link name="walls-collision-link">
        <collision name="walls-collision">
          <geometry>
            <mesh>
              <uri>../meshes/walls.dae</uri>
            </mesh>
          </geometry>
          <surface>
            <friction>
              <ode>
                <mu>100</mu>
                <mu2>50</mu2>
              </ode>
            </friction>
          </surface>
        </collision>

        <sensor name="walls-contact-sensor" type="contact">
          <contact>
            <collision>walls-collision</collision>
          </contact>
        </sensor>
      </link>

      <link name="decoration-collision-link">
        <collision name="decoration-collision">
          <geometry>
            <mesh>
              <uri>../meshes/decoration_colliders_2.dae</uri>
            </mesh>
          </geometry>
          <surface>
            <friction>
              <ode>
                <mu>100</mu>
                <mu2>50</mu2>
              </ode>
            </friction>
          </surface>
        </collision>

        <sensor name="decoration-contact-sensor" type="contact">
          <contact>
            <collision>decoration-collision</collision>
          </contact>
        </sensor>
      </link>
    </model>

    <!-- ========================= CAR ========================= -->
    <model name="car">
      <pose>1 0 0.07 0 0 0</pose>

            <link name='front_left_wheel'>
                <pose>0.110227 0.125006 -0.004396 -1.5707 0 0</pose>
                <inertial>
                <mass>2</mass>
                <inertia>
                    <ixx>0.145833</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>0.145833</iyy>
                    <iyz>0</iyz>
                    <izz>0.125</izz>
                </inertia>
                </inertial>
                <visual name='visual'>
                <geometry>
                    <cylinder>
                    <length>0.03</length>
                    <radius>0.056098</radius>
                    </cylinder>
                </geometry>
                <material>
                    <ambient>0.2 0.2 0.2 1</ambient>
                    <diffuse>0.2 0.2 0.2 1</diffuse>
                    <specular>0.2 0.2 0.2 1</specular>
                </material>
                </visual>
                <collision name='collision'>
                <geometry>
                    <cylinder>
                    <length>0.03</length>
                    <radius>0.056098</radius>
                    </cylinder>
                </geometry>
                <surface>
                    <friction>
                    <ode>
                        <mu>1.2</mu>
                        <mu2>1.2</mu2>
                        <fdir1>0 0 1</fdir1>
                    </ode>
                    </friction>
                </surface>
                </collision>
            </link>

            <link name='rear_left_wheel'>
                <pose>-0.190341 0.125006 -0.004396 -1.5707 0 0</pose>
                <inertial>
                <mass>2</mass>
                <inertia>
                    <ixx>0.145833</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>0.145833</iyy>
                    <iyz>0</iyz>
                    <izz>0.125</izz>
                </inertia>
                </inertial>
                <visual name='visual'>
                <geometry>
                    <cylinder>
                    <length>0.03</length>
                    <radius>0.056098</radius>
                    </cylinder>
                </geometry>
                <material>
                    <ambient>0.2 0.2 0.2 1</ambient>
                    <diffuse>0.2 0.2 0.2 1</diffuse>
                    <specular>0.2 0.2 0.2 1</specular>
                </material>
                </visual>
                <collision name='collision'>
                <geometry>
                    <cylinder>
                    <length>0.03</length>
                    <radius>0.056098</radius>
                    </cylinder>
                </geometry>
                <surface>
                    <friction>
                    <ode>
                        <mu>1.2</mu>
                        <mu2>1.2</mu2>
                        <fdir1>0 0 1</fdir1>
                    </ode>
                    </friction>
                </surface>
                </collision>
            </link>

            <link name='front_right_wheel'>
                <pose>0.110227 -0.125006 -0.004396 -1.5707 0 0</pose>
                <inertial>
                <mass>2</mass>
                <inertia>
                    <ixx>0.145833</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>0.145833</iyy>
                    <iyz>0</iyz>
                    <izz>0.125</izz>
                </inertia>
                </inertial>
                <visual name='visual'>
                <geometry>
                    <cylinder>
                    <length>0.03</length>
                    <radius>0.056098</radius>
                    </cylinder>
                </geometry>
                <material>
                    <ambient>0.2 0.2 0.2 1</ambient>
                    <diffuse>0.2 0.2 0.2 1</diffuse>
                    <specular>0.2 0.2 0.2 1</specular>
                </material>
                </visual>
                <collision name='collision'>
                <geometry>
                    <cylinder>
                    <length>0.03</length>
                    <radius>0.056098</radius>
                    </cylinder>
                </geometry>
                <surface>
                    <friction>
                    <ode>
                        <mu>1.2</mu>
                        <mu2>1.2</mu2>
                        <fdir1>0 0 1</fdir1>
                    </ode>
                    </friction>
                </surface>
                </collision>
            </link>

            <link name='rear_right_wheel'>
                <pose>-0.190341 -0.125006 -0.004396 -1.5707 0 0</pose>
                <inertial>
                <mass>2</mass>
                <inertia>
                    <ixx>0.145833</ixx>
                    <ixy>0</ixy>
                    <ixz>0</ixz>
                    <iyy>0.145833</iyy>
                    <iyz>0</iyz>
                    <izz>0.125</izz>
                </inertia>
                </inertial>
                <visual name='visual'>
                <geometry>
                    <cylinder>
                    <length>0.03</length>
                    <radius>0.056098</radius>
                    </cylinder>
                </geometry>
                <material>
                    <ambient>0.2 0.2 0.2 1</ambient>
                    <diffuse>0.2 0.2 0.2 1</diffuse>
                    <specular>0.2 0.2 0.2 1</specular>
                </material>
                </visual>
                <collision name='collision'>
                <geometry>
                    <cylinder>
                    <length>0.03</length>
                    <radius>0.056098</radius>
                    </cylinder>
                </geometry>
                <surface>
                    <friction>
                    <ode>
                        <mu>1.2</mu>
                        <mu2>1.2</mu2>
                        <fdir1>0 0 1</fdir1>
                    </ode>
                    </friction>
                </surface>
                </collision>
            </link>

        <!-- Visible Lidar puck -->
        <visual name="lidar_visual">
          <pose>0.08 0 0.07 0 0 0</pose>
          <geometry>
            <cylinder>
              <radius>0.03</radius>
              <length>0.04</length>
            </cylinder>
          </geometry>
        </visual>

        <!-- ONE lidar sensor mounted on chassis -->
        <sensor name="ust10lx" type="ray">
      
          <pose>0.08 0 0.07 0 0 0</pose>
            <topic>/scan</topic>
          <update_rate>40</update_rate>
          <always_on>true</always_on>
          <visualize>true</visualize>

            <joint name="front_left_wheel_steering_joint" type="revolute">
                <child>front_left_wheel_steering_link</child>
                <parent>chassis</parent>
                <axis>
                <xyz>0 0 1</xyz>
                <limit>
                    <lower>-0.7</lower>
                    <upper>+0.7</upper>
                    <velocity>1.0</velocity>
                    <effort>25</effort>
                </limit>
                <use_parent_model_frame>1</use_parent_model_frame>
                </axis>
            </joint>

            <joint name="front_right_wheel_steering_joint" type="revolute">
                <parent>chassis</parent>
                <child>front_right_wheel_steering_link</child>
                <axis>
                <xyz>0 0 1</xyz>
                <limit>
                    <lower>-0.7</lower>
                    <upper>+0.7</upper>
                    <velocity>1.0</velocity>
                    <effort>25</effort>
                </limit>
                </axis>
            </joint>

            <joint name='front_left_wheel_joint' type='revolute'>
                <parent>front_left_wheel_steering_link</parent>
                <child>front_left_wheel</child>
                <axis>
                <xyz>0 1 0</xyz>
                <use_parent_model_frame>1</use_parent_model_frame>
                <limit>
                    <lower>-1.79769e+308</lower>
                    <upper>1.79769e+308</upper>
                </limit>
                </axis>
            </joint>

            <joint name='front_right_wheel_joint' type='revolute'>
                <parent>front_right_wheel_steering_link</parent>
                <child>front_right_wheel</child>
                <axis>
                <xyz>0 1 0</xyz>
                <use_parent_model_frame>1</use_parent_model_frame>
                <limit>
                    <lower>-1.79769e+308</lower>
                    <upper>1.79769e+308</upper>
                </limit>
                </axis>
            </joint>

            <joint name='rear_left_wheel_joint' type='revolute'>
                <parent>chassis</parent>
                <child>rear_left_wheel</child>
                <axis>
                <xyz>0 1 0</xyz>
                <use_parent_model_frame>1</use_parent_model_frame>
                <limit>
                    <lower>-1.79769e+308</lower>
                    <upper>1.79769e+308</upper>
                </limit>
                </axis>
            </joint>

            <joint name='rear_right_wheel_joint' type='revolute'>
                <parent>chassis</parent>
                <child>rear_right_wheel</child>
                <axis>
                <xyz>0 1 0</xyz>
                <use_parent_model_frame>1</use_parent_model_frame>
                <limit>
                    <lower>-1.79769e+308</lower>
                    <upper>1.79769e+308</upper>
                </limit>
                </axis>
            </joint>

      <!-- ===================== STEERING LINKS ===================== -->
      <link name="front_left_wheel_steering_link">
        <pose>0.110227 0.1 0.003517 0 0 0</pose>
        <inertial>
          <mass>0.5</mass>
          <inertia>
            <ixx>0.0153</ixx>
            <iyy>0.025</iyy>
            <izz>0.0153</izz>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyz>0</iyz>
          </inertia>
        </inertial>
        <visual name="steering_link_visual">
          <geometry>
            <cylinder><length>0.017583</length><radius>0.005983</radius></cylinder>
          </geometry>
          <material>
            <ambient>1 1 1</ambient>
            <diffuse>1 1 1</diffuse>
          </material>
        </visual>
      </link>

            <!-- ACKERMANN STEERING PLUGIN -->
            <plugin
                filename="ignition-gazebo-ackermann-steering-system"
                name="gz::sim::systems::AckermannSteering">
                <left_joint>front_left_wheel_joint</left_joint>
                <left_joint>rear_left_wheel_joint</left_joint>
                <right_joint>front_right_wheel_joint</right_joint>
                <right_joint>rear_right_wheel_joint</right_joint>
                <left_steering_joint>front_left_wheel_steering_joint</left_steering_joint>
                <right_steering_joint>front_right_wheel_steering_joint</right_steering_joint>
                <kingpin_width>0.2</kingpin_width>
                <steering_limit>0.7</steering_limit>
                <wheel_base>0.300568</wheel_base>
                <wheel_separation>0.250012</wheel_separation>
                <wheel_radius>0.056098</wheel_radius>
                <min_velocity>-1</min_velocity>
                <max_velocity>3.0</max_velocity>
                <min_acceleration>-3.0</min_acceleration>
                <max_acceleration>3.0</max_acceleration>
            </plugin>

      <!-- ===================== JOINTS ===================== -->
      <joint name="front_left_wheel_steering_joint" type="revolute">
        <parent>chassis</parent>
        <child>front_left_wheel_steering_link</child>
        <axis>
          <xyz>0 0 1</xyz>
          <limit>
            <lower>-0.6</lower>
            <upper>0.6</upper>
            <velocity>1.0</velocity>
            <effort>25</effort>
          </limit>
          <use_parent_model_frame>1</use_parent_model_frame>
        </axis>
      </joint>

      <joint name="front_right_wheel_steering_joint" type="revolute">
        <parent>chassis</parent>
        <child>front_right_wheel_steering_link</child>
        <axis>
          <xyz>0 0 1</xyz>
          <limit>
            <lower>-0.6</lower>
            <upper>0.6</upper>
            <velocity>1.0</velocity>
            <effort>25</effort>
          </limit>
        </axis>
      </joint>

      <joint name="front_left_wheel_joint" type="revolute">
        <parent>front_left_wheel_steering_link</parent>
        <child>front_left_wheel</child>
        <axis>
          <xyz>0 0 1</xyz>
          <limit>
            <lower>-1.79769e+308</lower>
            <upper>1.79769e+308</upper>
          </limit>
        </axis>
      </joint>

      <joint name="front_right_wheel_joint" type="revolute">
        <parent>front_right_wheel_steering_link</parent>
        <child>front_right_wheel</child>
        <axis>
          <xyz>0 0 1</xyz>
          <limit>
            <lower>-1.79769e+308</lower>
            <upper>1.79769e+308</upper>
          </limit>
        </axis>
      </joint>

      <joint name="rear_left_wheel_joint" type="revolute">
        <parent>chassis</parent>
        <child>rear_left_wheel</child>
        <axis>
          <xyz>0 0 1</xyz>
          <limit>
            <lower>-1.79769e+308</lower>
            <upper>1.79769e+308</upper>
          </limit>
        </axis>
      </joint>

      <joint name="rear_right_wheel_joint" type="revolute">
        <parent>chassis</parent>
        <child>rear_right_wheel</child>
        <axis>
          <xyz>0 0 1</xyz>
          <limit>
            <lower>-1.79769e+308</lower>
            <upper>1.79769e+308</upper>
          </limit>
        </axis>
      </joint>

      <!-- ===================== ACKERMANN PLUGIN ===================== -->
      <plugin filename="ignition-gazebo-ackermann-steering-system"
              name="gz::sim::systems::AckermannSteering">

        <!-- Use REAR wheels for drive -->
        <left_joint>rear_left_wheel_joint</left_joint>
        <right_joint>rear_right_wheel_joint</right_joint>

        <!-- Use front steering joints for steering -->
        <left_steering_joint>front_left_wheel_steering_joint</left_steering_joint>
        <right_steering_joint>front_right_wheel_steering_joint</right_steering_joint>

        <kingpin_width>0.2</kingpin_width>
        <steering_limit>0.5</steering_limit>
        <wheel_base>0.198864</wheel_base>
        <wheel_separation>0.25</wheel_separation>
        <wheel_radius>0.056098</wheel_radius>

        <min_velocity>-1</min_velocity>
        <max_velocity>1</max_velocity>
        <min_acceleration>-3</min_acceleration>
        <max_acceleration>3</max_acceleration>
      </plugin>

    </model>

    <!-- ===================== SYSTEM PLUGINS ===================== -->
<plugin filename="gz-sim-sensors-system"
        name="gz::sim::systems::Sensors">
  <render_engine>ogre2</render_engine>
</plugin>

    <plugin filename="gz-sim-physics-system"
            name="gz::sim::systems::Physics" />
    <plugin filename="gz-sim-user-commands-system"
            name="gz::sim::systems::UserCommands" />
    <plugin filename="gz-sim-scene-broadcaster-system"
            name="gz::sim::systems::SceneBroadcaster" />

  </world>
</sdf>